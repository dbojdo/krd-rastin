{
    "name": "goosfraba/krd-rastin-sdk",
    "description": "The SDK for KRD Rastin 2.0 protocol",
    "keywords": ["krd", "rastin", "rastin 2.0", "sdk", "api"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "webit/soap-api": "^3.0.0",
        "jms/serializer": "^3.23",
        "phpspec/prophecy-phpunit": "^2.1",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Goosfraba\\KrdRastin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goosfraba\\KrdRastin\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
